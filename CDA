------> Váriaveis <------
    
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("TknmX - Cidade Alta", "Ocean")
    local Credito = Window:NewTab("Créditos")
    local Server = Window:NewTab("Server")
    local Client = Window:NewTab("Client")
    local Outros = Window:NewTab("Outros")
    local Principal = Credito:NewSection("Créditos aos criadores")
    local Okei = Server:NewSection("Scripts executaveis ao Server")
    local Bruh = Outros:NewSection("Funções do Menu")
    local eae = "https://discord.gg/ug3bqTS"
    local Tk = Client:NewSection("Scripts executaveis ao Client")
    
    ------> Funções <------
    
    Principal:NewButton("Scripts By : Drezin#7409", "Este botão não possui nenhuma funcionabilidade !")
    Principal:NewButton("Youtube : TknmX Oficial", "Este botão não possui nenhuma funcionabilidade !")
    Principal:NewButton("Discord : Click para Copiar", "Este botão faz você copiar o nosso Discord !", function()
    setclipboard(tostring(eae))
    game.StarterGui:SetCore("SendNotification", {
    Title = "TknmX - Avisos";
    Text = "Discord copiado com sucesso!";
    Duration = 5;
    })
    end)
    
    ------> Kill All <------
    
    Okei:NewButton("Kill All", "Mate todos os Jogadores", function()
    -- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide
    for i,v in pairs(game.Workspace:GetChildren()) do
        if game.Players:FindFirstChild(v.Name) then
    local ohInstance1 = workspace[v.Name].Humanoid
    local ohNumber2 = 101
    local ohNumber3 = 0
    local ohNumber4 = 0
    
    game:GetService("ReplicatedStorage").ACS_Engine.Eventos.Damage:FireServer(ohInstance1, ohNumber2, ohNumber3, ohNumber4)
    end
    end
    end)
    
    ------> Dinheiro <------
    
    Okei:NewButton("Lagar Servidor", "Spawne muitos carros", function()
    -- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide
    
    local ohString1 = "Ram"
    
    game:GetService("ReplicatedStorage").SpawnCar:FireServer(ohString1)
    
    -- This script was generated by Hydroxide's RemoteSpy: https://github.com/Upbolt/Hydroxide
    
    local ohString1 = "Caprice"
    
    game:GetService("ReplicatedStorage").SpawnCar:FireServer(ohString1)
    end)
    
